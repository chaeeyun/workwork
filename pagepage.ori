	var win = window;    // 찾고자 하는 윈도우
	var n   = 0;
 
	function findInPage(str) {
	 
	  var txt, i, found;
	 
	  if (str == "")
	    return false;
	 
	  // 페이지 내에서 찾고자 하는 문자열이 다음 나오는 곳을 찾는다
