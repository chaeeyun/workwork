	  for (i = 0; i <= n && (found = txt.findText(str)) != false; i++) {
	      txt.moveStart("character", 1);
	      txt.moveEnd("textedit");
	  }
	 
	  // 발견하면 그것을 반전시키고, 그 위치로 스크롤시킨다.	 
	  if (found) {
	      txt.moveStart("character", -1);
	      txt.findText(str);
	      txt.select();
	      txt.scrollIntoView();
	      n++;
	  }
	 
	  //그렇지 않으면 페이지의 처음부터 찾는다.	 
	  else {
	      if (n > 0) {
	        n = 0;
	        findInPage(str);
